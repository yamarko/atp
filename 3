#include <stdio.h>

int main()
{
int d;
printf("Vvedit chyslo vid 1 do 7:\n");
scanf("%d", &d);
switch (d) {
case 1: printf("Ponedilok\n"); break;
case 2: printf("Vivtorok\n"); break;
case 3: printf("Sereda\n"); break;
case 4: printf("Chetver\n"); break;
case 5: printf("Piatnytsia\n"); break;
case 6: printf("Subota\n"); break;
case 7: printf("Nedilia\n"); break;
default: printf("Vvedit chyslo vid 1 do 7"); break;
}
}
